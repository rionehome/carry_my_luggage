<?xml version="1.0"?>

<launch>
  <include file="$(find turtlebot_bringup)/launch/minimal.launch"/>

 <!-- open_manipulator_controllerもlaunchファイルから起動できるが終了するときにノードが終了するので別の端末で手動で起動するのをすすめる -->
 <!-- <include file="$(find open_manipulator_controller)/launch/open_manipulator_controller.launch"/> -->

  <include file="$(find rplidar_ros)/launch/rplidar.launch"/>

  <node pkg="carry_my_luggage" name="main" type="main.py" required="true" output="screen" />
  <node pkg="carry_my_luggage" name="move" type="move.py" required="true" output="screen" />
  <!--<node pkg="carry_my_luggage" name="arm" type="arm.py" required="true" output="screen" />-->
  <node pkg="carry_my_luggage" name="audio" type="audio.py" required="true" output="screen" />
  <node pkg="carry_my_luggage" name="lidar" type="lidar.py" required="true" output="screen" />
</launch>